<% provide(:title, "Sign in") %>
<div class="test">Sign in</div>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(:user_session, url: admin_user_sessions_path) do |f| %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Sign in", class: "btn btn-large btn-primary" %>
    <% end %>

  </div>
</div>